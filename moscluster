--
-- Name: events; Type: TABLE; Schema: public; Owner: moscluster
--

CREATE TABLE public.events (
    id integer NOT NULL,
    image_id integer,
    user_id uuid,
    title character varying(500) NOT NULL,
    abstract text,
    content text NOT NULL,
    raw_content text NOT NULL,
    content_formatter character varying(255) NOT NULL,
    enabled boolean NOT NULL,
    slug character varying(255) NOT NULL,
    publication_date_start timestamp(0) without time zone DEFAULT NULL::timestamp without time zone,
    comments_enabled boolean NOT NULL,
    comments_close_at timestamp(0) without time zone DEFAULT NULL::timestamp without time zone,
    comments_default_status integer NOT NULL,
    comments_count integer,
    created_at timestamp(0) without time zone NOT NULL,
    updated_at timestamp(0) without time zone NOT NULL,
    dnpp_id integer,
    oii_id integer,
    address character varying(300) DEFAULT NULL::character varying,
    site character varying(300) DEFAULT NULL::character varying,
    costtype integer,
    datebegin timestamp(0) without time zone DEFAULT NULL::timestamp without time zone,
    dateend timestamp(0) without time zone DEFAULT NULL::timestamp without time zone,
    lang character varying(5) DEFAULT NULL::character varying,
    imoscow_id integer,
    category_id integer,
    contact character varying(300) DEFAULT NULL::character varying,
    source_link character varying(255) DEFAULT NULL::character varying,
    contact_fio character varying(300) DEFAULT NULL::character varying,
    metro character varying(300) DEFAULT NULL::character varying,
    email character varying(300) DEFAULT NULL::character varying,
    status integer,
    address_coord character varying(255) DEFAULT NULL::character varying,
    lat numeric(10,8),
    lon numeric(10,8),
    phone character varying(255)
);


ALTER TABLE public.events OWNER TO moscluster;

