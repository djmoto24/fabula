--
-- Name: log; Type: TABLE; Schema: public; Owner: moscluster
--

CREATE TABLE public.log (
    id uuid NOT NULL,
    message text NOT NULL,
    context text NOT NULL,
    level smallint NOT NULL,
    level_name character varying(50) NOT NULL,
    extra text NOT NULL,
    created_at timestamp(0) without time zone NOT NULL,
    log_type smallint,
    user_fio text,
    user_name character varying(700) DEFAULT NULL::character varying,
    user_email character varying(200) DEFAULT NULL::character varying,
    user_company character varying(1000) DEFAULT NULL::character varying,
    user_companyid integer,
    user_sudir character varying(50) DEFAULT NULL::character varying,
    user_sk character varying(100) DEFAULT NULL::character varying,
    request_type integer
);


ALTER TABLE public.log OWNER TO moscluster;

