--
-- Name: exchange; Type: TABLE; Schema: public; Owner: moscluster
--

CREATE TABLE public.exchange (
    id integer NOT NULL,
    company_id integer NOT NULL,
    parameters text,
    date_create timestamp(0) without time zone NOT NULL,
    date_update timestamp(0) without time zone NOT NULL,
    status integer,
    hidden boolean,
    comment text,
    query character varying(255) DEFAULT NULL::character varying,
    number integer,
    type_text character varying(255) NOT NULL,
    type integer NOT NULL,
    discr character varying(255) NOT NULL,
    length integer,
    width integer,
    height integer,
    diameter integer,
    wall integer,
    weight integer,
    tableting integer,
    powder_forms integer,
    liquid_forms integer,
    length_unit integer,
    width_unit integer,
    height_unit integer,
    diameter_unit integer,
    wall_unit integer,
    date_public timestamp(0) without time zone DEFAULT NULL::timestamp without time zone,
    point integer,
    phone character varying(255) DEFAULT NULL::character varying,
    email character varying(255) DEFAULT NULL::character varying,
    ias_status integer,
    ias_guid character varying(255) DEFAULT NULL::character varying,
    confirm boolean,
    date_get_guid timestamp(0) without time zone DEFAULT NULL::timestamp without time zone,
    ias_order_guid character varying(255) DEFAULT NULL::character varying,
    date_get_order_guid timestamp(0) without time zone DEFAULT NULL::timestamp without time zone,
    lastname character varying(600) DEFAULT NULL::character varying,
    firstname character varying(600) DEFAULT NULL::character varying,
    middlename character varying(600) DEFAULT NULL::character varying,
    date_get_ias_result timestamp(0) without time zone DEFAULT NULL::timestamp without time zone
);


ALTER TABLE public.exchange OWNER TO moscluster;

