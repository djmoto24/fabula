--

CREATE TABLE public.request_in_cluster (
    id integer NOT NULL,
    company integer NOT NULL,
    name character varying(255) NOT NULL,
    first_name character varying(255) NOT NULL,
    "position" character varying(255) NOT NULL,
    phone character varying(255) NOT NULL,
    email character varying(255) NOT NULL,
    date_create timestamp(0) without time zone NOT NULL,
    inn character varying(255) DEFAULT NULL::character varying,
    kpp character varying(255) DEFAULT NULL::character varying,
    legal_address character varying(255) DEFAULT NULL::character varying,
    actual_address character varying(255) DEFAULT NULL::character varying,
    not_bankrupt boolean,
    not_suspended boolean,
    ias_guid character varying(255) DEFAULT NULL::character varying,
    middle_name character varying(255) DEFAULT NULL::character varying,
    last_name character varying(255) NOT NULL,
    confirm boolean,
    status character varying(255) DEFAULT NULL::character varying
);


