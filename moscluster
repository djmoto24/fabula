--
-- Name: make_tsvector(text, text); Type: FUNCTION; Schema: public; Owner: moscluster
--

CREATE FUNCTION public.make_tsvector(title text, content text) RETURNS tsvector
    LANGUAGE plpgsql IMMUTABLE
    AS $$
BEGIN
  RETURN (setweight(to_tsvector('russian', coalesce(title, '')),'A') ||
    setweight(to_tsvector('russian', coalesce(content, '')), 'B'));
END
$$;


ALTER FUNCTION public.make_tsvector(title text, content text) OWNER TO moscluster;

--
-- Name: FUNCTION make_tsvector(title text, content text); Type: COMMENT; Schema: public; Owner: moscluster
--

COMMENT ON FUNCTION public.make_tsvector(title text, content text) IS 'Принимает заголовок и контент и возвращает соответствующий ей tsvector. У заголовков вес будет больше.';


SET default_tablespace = '';

SET default_with_oids = false;

