--
-- Name: users; Type: TABLE; Schema: public; Owner: moscluster
--

CREATE TABLE public.users (
    id uuid NOT NULL,
    email character varying(180),
    roles text NOT NULL,
    lastname character varying(200),
    firstname character varying(200),
    middlename character varying(200),
    phone character varying(30),
    company character varying(500),
    ogrn character varying(30),
    inn character varying(30),
    access_token character varying(200),
    expires integer,
    refresh_token character varying(200) DEFAULT NULL::character varying,
    elk_authed boolean,
    ias_access_token character varying(50) DEFAULT NULL::character varying,
    company_position character varying(500) DEFAULT NULL::character varying,
    ias_companyid integer,
    fake boolean,
    trusted_company integer,
    admin_access boolean,
    created timestamp(0) without time zone,
    lastlogin timestamp(0) without time zone,
    last_login timestamp(0) without time zone DEFAULT NULL::timestamp without time zone,
    confirmation_token character varying(180) DEFAULT NULL::character varying,
    password_requested_at timestamp(0) without time zone DEFAULT NULL::timestamp without time zone,
    username character varying(180) DEFAULT NULL::character varying,
    username_canonical character varying(180) DEFAULT NULL::character varying,
    email_canonical character varying(180) DEFAULT NULL::character varying,
    salt character varying(255) DEFAULT NULL::character varying,
    password character varying(255) DEFAULT NULL::character varying,
    enabled boolean,
    preregistraion boolean,
    preregistraion_informed boolean,
    sk_id character varying(100) DEFAULT NULL::character varying,
    sk_expired integer,
    sudir_guid character varying(50) DEFAULT NULL::character varying,
    content_editor boolean,
    fond_editor boolean,
    sudir_token_id text,
    project_editor boolean,
    ias_guid character varying(50) DEFAULT NULL::character varying,
    content_project_editor boolean,
    partner_informed boolean,
    is_pass_covid boolean,
    fake_list_access boolean
);


ALTER TABLE public.users OWNER TO moscluster;

