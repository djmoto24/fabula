--
-- Name: contract; Type: TABLE; Schema: public; Owner: moscluster
--

CREATE TABLE public.contract (
    id integer NOT NULL,
    number character varying(255) NOT NULL,
    name text NOT NULL,
    date_create timestamp(0) without time zone NOT NULL,
    date_end timestamp(0) without time zone DEFAULT NULL::timestamp without time zone,
    monthly_average numeric(50,2),
    delay integer,
    status integer NOT NULL,
    date_contract timestamp(0) without time zone NOT NULL,
    deleted boolean,
    date_delete timestamp(0) without time zone DEFAULT NULL::timestamp without time zone,
    hash character varying(255) DEFAULT NULL::character varying,
    date_send_in_blockchain timestamp(0) without time zone DEFAULT NULL::timestamp without time zone,
    hash_status boolean,
    date_hash_status_update timestamp(0) without time zone DEFAULT NULL::timestamp without time zone,
    creator_id integer,
    showed boolean
);


ALTER TABLE public.contract OWNER TO moscluster;

