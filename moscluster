--
-- Name: exchange_offer; Type: TABLE; Schema: public; Owner: moscluster
--

CREATE TABLE public.exchange_offer (
    id integer NOT NULL,
    exchange_id integer NOT NULL,
    company_id integer NOT NULL,
    price numeric(50,2) DEFAULT NULL::numeric,
    date_completion timestamp(0) without time zone DEFAULT NULL::timestamp without time zone,
    works text,
    date_create timestamp(0) without time zone NOT NULL,
    status integer,
    comment text NOT NULL,
    showed boolean
);


ALTER TABLE public.exchange_offer OWNER TO moscluster;

