--
-- Name: company; Type: TABLE; Schema: public; Owner: moscluster
--

CREATE TABLE public.company (
    id integer NOT NULL,
    name character varying(1000) NOT NULL,
    full_name character varying(1000) DEFAULT NULL::character varying,
    inn character varying(20) DEFAULT NULL::character varying,
    address character varying(1000) DEFAULT NULL::character varying,
    percent_complete integer NOT NULL,
    image_id integer,
    kpp character varying(20) DEFAULT NULL::character varying,
    legal_address character varying(1000) DEFAULT NULL::character varying,
    name_fts tsvector NOT NULL,
    ogrn character varying(20) DEFAULT NULL::character varying,
    is_cluster_member boolean,
    lat character varying(255),
    lon character varying(255),
    type character varying(255),
    logo character varying(255),
    okved character varying(255),
    is_moderated_by_external_admin boolean,
    partner_custer boolean,
    partner_cluster_date timestamp(0) without time zone DEFAULT NULL::timestamp without time zone,
    percent_update timestamp(0) without time zone DEFAULT NULL::timestamp without time zone
);


ALTER TABLE public.company OWNER TO moscluster;

