    date_update timestamp(0) without time zone NOT NULL,
    last_notification_request timestamp(0) without time zone DEFAULT NULL::timestamp without time zone,
    change_status_date timestamp(0) without time zone DEFAULT NULL::timestamp without time zone,
    new_status boolean
);


