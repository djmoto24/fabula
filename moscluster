    doc_serial character varying(20) DEFAULT NULL::character varying,
    doc_number character varying(140) NOT NULL,
    doc_date character varying(120) DEFAULT NULL::character varying,
    doc_ved character varying(350) DEFAULT NULL::character varying,
    auto_mark character varying(200) DEFAULT NULL::character varying,
    auto_model character varying(200) DEFAULT NULL::character varying,
    auto_number character varying(200) DEFAULT NULL::character varying,
    troika_number character varying(50) DEFAULT NULL::character varying,
    strelka_number character varying(50) DEFAULT NULL::character varying,
    social_card_number character varying(50) DEFAULT NULL::character varying,
    trans_ticket_number character varying(50) DEFAULT NULL::character varying,
    sends_count integer,
    last_status integer,
    last_status_text character varying(850) DEFAULT NULL::character varying,
