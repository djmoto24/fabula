    price real,
    room_types character varying(255),
    company text,
    main_photo text,
    photos text,
    building_photo text,
    rate_type character varying(255),
    cost_type character varying(255),
    description text,
    address character varying(255),
    public_status character varying(255),
    sorting_number integer
);


