--
-- Name: inno_object; Type: TABLE; Schema: public; Owner: moscluster
--

CREATE TABLE public.inno_object (
    id integer NOT NULL,
    name text NOT NULL,
    address character varying(255) DEFAULT NULL::character varying,
    description text,
    short_description character varying(255) DEFAULT NULL::character varying,
    fio character varying(255) DEFAULT NULL::character varying,
    website character varying(255) DEFAULT NULL::character varying,
    phone text,
    email character varying(255) DEFAULT NULL::character varying,
    lat character varying(255) DEFAULT NULL::character varying,
    lon character varying(255) DEFAULT NULL::character varying,
    logo_url character varying(255) DEFAULT NULL::character varying,
    inno_object_logo_url character varying(255) DEFAULT NULL::character varying,
    presentation_url character varying(255) DEFAULT NULL::character varying,
    deleted boolean,
    visitors character varying(255) DEFAULT NULL::character varying,
    equipment_use text,
    employees_number integer,
    image_id integer,
    ias_id character varying(255) NOT NULL,
    lang character varying(5) DEFAULT NULL::character varying,
    inno_object_type_id_id integer NOT NULL,
    inno_object_status_id integer,
    district_id integer,
    region_id integer,
    preview_url character varying(255) DEFAULT NULL::character varying,
    image_url character varying(255) DEFAULT NULL::character varying,
    ias_image_original_url character varying(255) DEFAULT NULL::character varying,
    ias_image_preview_url character varying(255) DEFAULT NULL::character varying,
    ias_image_image_url character varying(255) DEFAULT NULL::character varying,
    ias_image_comment character varying(255) DEFAULT NULL::character varying,
    cluster_role_id_id integer,
    company_id integer,
    metro character varying(255) DEFAULT NULL::character varying,
    company_short_name text,
    company_name text,
    company_inn character varying(255) DEFAULT NULL::character varying,
    company_okfs text,
    technopark_status integer,
    company_status boolean,
    ispartner boolean
);


ALTER TABLE public.inno_object OWNER TO moscluster;

